# Lizmap directories - adjust these paths for your deployment
LIZMAP_PROJECTS=./projects
LIZMAP_DIR=./lizmap

# User/Group IDs - should match the user running docker
LIZMAP_UID=1000
LIZMAP_GID=1000

# Docker registry for lizmap-web-client image
# Options: dimasciput, kartoza (when available), 3liz
DOCKER_REGISTRY=dimasciput

# Docker registry for qgis-map-server image (defaults to 3liz if not set)
# Note: dimasciput may not have all QGIS versions available
# QGIS_REGISTRY=3liz

# Image versions
LIZMAP_VERSION_TAG=3.8
QGIS_VERSION_TAG=3.34
POSTGIS_VERSION=17-3

# Database
POSTGRES_PASSWORD=postgres

# Workers
QGIS_MAP_WORKERS=4
WPS_NUM_WORKERS=1

# Ports
LIZMAP_PORT=8090
OWS_PORT=127.0.0.1:8091
WPS_PORT=127.0.0.1:8092
MAPPROXY_PORT=8092
POSTGIS_PORT=127.0.0.1:5432
POSTGIS_ALIAS=db.lizmap

# Mapproxy
PRODUCTION=TRUE
PROCESSES=6
THREADS=10
MAPPROXY_USER_ID=1000
MAPPROXY_GROUP_ID=1000
